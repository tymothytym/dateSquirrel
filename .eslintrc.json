{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "linebreak-style": ["error", "windows"],
        "quotes": "off",
        "semi": ["error", "always"]
    }
}